<html class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">
    <meta name="description" content="">
    <meta name="robots" content="{$robots}" n:ifset="$robots">

    <title>{#title|striptags|strip|trim}Nette Framework application{/#}</title>

    <!--[if lt IE 9]><script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <style>
        @import url({$baseUrl}{='css/bootstrap.css'|mtime});
        @import url({$baseUrl}{='css/screen.css'|mtime});
    </style>
    <link rel="stylesheet" media="print" href="{$basePath}{='css/print.css'|mtime}">
    {#head}{/#}

    {*{control ga "UA-XXX"}*}
</head>
<body>
    <div class="container" n:block="body">
        {#flashMessages}{control flashes}{/#}
        {include #content}
    </div>

    {#scripts}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="' + {$basePath} + {='js/jquery.js'|mtime} + '"><\/script>')</script>
    <script src="{$basePath}{='js/modernizr.js'|mtime}"></script>
    <script src="{$basePath}{='js/bootstrap.js'|mtime}"></script>
    <script src="{$basePath}{='js/nette.js'|mtime}"></script>
    <script src="{$basePath}{='js/main.js'|mtime}"></script>
    {*<script src="{$basePath}{='js/pie.js'|mtime}"></script>*}
    {*<script src="{$basePath}{='js/upload.js'|mtime}"></script>*}
    {/#}
</body>
</html>
